
CREATE TABLE `e_commerce`.`sub_categories` ( `id` INT NOT NULL AUTO_INCREMENT , `translation_lang` VARCHAR(10) NOT NULL , `translation_of` INT NOT NULL , `name` VARCHAR(199) NOT NULL , `slug` VARCHAR(150) NOT NULL , `photo` VARCHAR(190) NOT NULL , `active` TINYINT(1) NOT NULL DEFAULT '1' , `created_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , `updated_at` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP , PRIMARY KEY (`id`)) ENGINE = InnoDB;
